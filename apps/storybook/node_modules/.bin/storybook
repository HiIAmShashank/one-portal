#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/shashank/Code/one-portal-test/node_modules/.pnpm/storybook@9.1.13_@testing-library+dom@10.4.0_prettier@3.6.2_vite@6.4.1_@types+node@24.7_fed13ec4f33e50dacb661f57fec977a8/node_modules/storybook/bin/node_modules:/home/shashank/Code/one-portal-test/node_modules/.pnpm/storybook@9.1.13_@testing-library+dom@10.4.0_prettier@3.6.2_vite@6.4.1_@types+node@24.7_fed13ec4f33e50dacb661f57fec977a8/node_modules/storybook/node_modules:/home/shashank/Code/one-portal-test/node_modules/.pnpm/storybook@9.1.13_@testing-library+dom@10.4.0_prettier@3.6.2_vite@6.4.1_@types+node@24.7_fed13ec4f33e50dacb661f57fec977a8/node_modules:/home/shashank/Code/one-portal-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/shashank/Code/one-portal-test/node_modules/.pnpm/storybook@9.1.13_@testing-library+dom@10.4.0_prettier@3.6.2_vite@6.4.1_@types+node@24.7_fed13ec4f33e50dacb661f57fec977a8/node_modules/storybook/bin/node_modules:/home/shashank/Code/one-portal-test/node_modules/.pnpm/storybook@9.1.13_@testing-library+dom@10.4.0_prettier@3.6.2_vite@6.4.1_@types+node@24.7_fed13ec4f33e50dacb661f57fec977a8/node_modules/storybook/node_modules:/home/shashank/Code/one-portal-test/node_modules/.pnpm/storybook@9.1.13_@testing-library+dom@10.4.0_prettier@3.6.2_vite@6.4.1_@types+node@24.7_fed13ec4f33e50dacb661f57fec977a8/node_modules:/home/shashank/Code/one-portal-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../storybook/bin/index.cjs" "$@"
else
  exec node  "$basedir/../storybook/bin/index.cjs" "$@"
fi
